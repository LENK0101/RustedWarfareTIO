[turret_heal]
invisible: true
x: 0
y:0
idleDir:0
projectile: heal
shoot_sound: NONE
shoot_sound_vol: 0.8
turnSpeed:99999
turnSpeedAcceleration:999

[projectile_heal]
@copyFromSection: projectile_buff
tags: NONE
areaDamage: -100
spawnProjectilesOnExplode: NONE
mutator13_ifUnitWithTags: notThisTarget
mutator13_directDamageMultiplier: 0
mutator14_ifUnitWithTags: notThisTarget
mutator14_areaDamageMultiplier: 0

[projectile_restore1HP]
