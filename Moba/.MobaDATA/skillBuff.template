image_wreak:  NONE
image_turret: NONE
image_shadow: NONE
image_offsetX: 0
image_offsetY: 0
shadowOffsetX:0
shadowOffsetY:0
scaleImagesTo: 0

[turret_buff]
invisible: true
x: 0
y:0
idleDir:0
projectile: buff
shoot_sound: NONE
shoot_sound_vol: 0.8
turnSpeed:99999
turnSpeedAcceleration:999

[projectile_buff]
areaDamage:1
areaRadiusFromEdge: true
areaDamageNoFalloff: true
life:0
speed:999
explodeEffect:NONE
